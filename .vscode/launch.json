{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug pmt",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/pmt",
            "args": ["-spt", "build/sotest-ut", "-o", "-fdiagnostics-color=always -Werror -g -lsolib -L build/", "-f", "tests/sotest.cpp", "tests/solib.hpp", "-pt", "build/libsolib.so", "-o", "-fdiagnostics-color=always -Werror -g -fpic -shared", "-f", "tests/solib.hpp", "tests/solib.cpp", "-u", "UNITTESTKEY", "-l", "5"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
              {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
              }
            ],
            "preLaunchTask": "g++ via comp.sh"
          }
    ]
}